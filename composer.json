{
  "name": "kaishiyoku/hera-rss-crawler",
  "description": "Modern library to handle RSS/Atom feeds",
  "homepage": "https://github.com/kaishiyoku/hera-rss-crawler",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de",
      "homepage": "https://andreas-wiedel.de"
    }
  ],
  "require": {
    "php": "^7.2.5",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "symfony/dom-crawler": "^5.2.1",
    "symfony/css-selector": "^5.2.1",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "nesbot/carbon": "^2.43.0",
    "laminas/laminas-xml": "^1.3",
    "laminas/laminas-feed": "^2.13",
    "monolog/monolog": "^2.2"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "^5.2.2",
    "symfony/yaml": "^5.2.1",
    "phpunit/phpunit": "^8.0|^9.3",
    "spatie/phpunit-snapshot-assertions": "^2.2|^4.2",
    "symfony/var-dumper": "^5.2.1",
    "mockery/mockery": "^1.3|^1.4",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\HeraRssCrawler\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/TestClasses/FailingTestClass.php"
    ]
  },
  "scripts": {
    "analyse": [
      ".\\vendor\\bin\\phpstan analyse"
    ]
  }
}
