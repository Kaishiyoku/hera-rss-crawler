{
  "name": "kaishiyoku/hera-rss-crawler",
  "description": "Modern library to handle RSS/Atom feeds",
  "homepage": "https://github.com/kaishiyoku/hera-rss-crawler",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de",
      "homepage": "https://andreas-wiedel.de"
    }
  ],
  "keywords": [
    "rss",
    "atom",
    "feed",
    "crawler",
    "parser",
    "reader"
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "symfony/dom-crawler": "^5.4.21|^6.2.7",
    "symfony/css-selector": "^5.4.21|^6.2.7",
    "guzzlehttp/guzzle": "^7.5.0",
    "illuminate/support": "^9.0|^10.0",
    "nesbot/carbon": "^2.66.0",
    "laminas/laminas-xml": "^1.5.0",
    "laminas/laminas-feed": "^2.20.0",
    "monolog/monolog": "^2.9.1|^3.3.1"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "^5.3.0",
    "symfony/yaml": "^5.4.21|^6.2.7",
    "phpunit/phpunit": "^9.6.4|^10.0.15",
    "spatie/phpunit-snapshot-assertions": "^4.2.16",
    "symfony/var-dumper": "^5.4.21|^6.2.7",
    "mockery/mockery": "^1.5.1",
    "phpstan/phpstan": "^1.10.6"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\HeraRssCrawler\\": "src/"
    }
  },
  "autoload-dev": {
    "files": [
      "tests/TestClasses/FailingTestClass.php"
    ]
  },
  "scripts": {
    "analyze": [
      ".\\vendor\\bin\\phpstan analyse"
    ]
  }
}
