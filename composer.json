{
  "name": "kaishiyoku/hera-rss-crawler",
  "description": "Modern library to handle RSS/Atom feeds",
  "homepage": "https://github.com/kaishiyoku/hera-rss-crawler",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de",
      "homepage": "https://andreas-wiedel.de"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "zendframework/zendxml": "1.2.0",
    "symfony/dom-crawler": "^5.0",
    "symfony/css-selector": "^5.0",
    "guzzlehttp/guzzle": "^6.5",
    "illuminate/support": "^6.8",
    "nesbot/carbon": "^2.28",
    "zendframework/zend-feed": "^2.12"
  },
  "require-dev": {
    "phpdocumentor/reflection-docblock": "4.3.2",
    "symfony/yaml": "4.4.1",
    "phpunit/phpunit": "7.5.1",
    "spatie/phpunit-snapshot-assertions": "2.0.0",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\HeraRssCrawler\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  }
}
